    "import turtle\n",
    "import random\n",
    " \n",
    "number = random.uniform(0, 3)\n",
    "\n",
    "pen = turtle.Turtle()\n",
    "pen.shape('circle')\n",
    " \n",
    "if number < 1.0:\n",
    "    pen.color('green')\n",
    "elif number < 2.0:\n",
    "    pen.color('yellow')\n",
    "elif number < 3.0:\n",
    "    pen.color('red')\n",
    "print(number)\n",
    "turtle.done()"
   ]
  }

    "import turtle\n",
    "pen = turtle.Turtle()\n",
    "pen.fillcolor('purple')\n",
    "pen.begin_fill()\n",
    "for i in range(2):\n",
    "    pen.fd(100)\n",
    "    pen.rt(90)\n",
    "    pen.fd(200)\n",
    "    pen.rt(90)\n",
    "pen.end_fill()\n",
    "turtle.done()"
   ]
